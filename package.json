{
  "name": "@paramlabs/backend",
  "version": "2.0.0",
  "private": true,
  "description": "Paramlabs backend API",
  "main": "src/index.js",
  "scripts": {
    "test": "mocha",
    "test:debug": "cross-env DEBUG=true npm test",
    "test:watch": "cross-env WATCH=true npm test",
    "test:watch:debug": "cross-env DEBUG=true WATCH=true npm test",
    "format": "prettier --write src/**/*.{js,json} && npm run lint -- --fix",
    "start": "cross-env DEBUG=app:* node .",
    "dev": "cross-env DEBUG=app:* nodemon --inspect .",
    "lint": "eslint .",
    "generate:schema": "hygen generator schema",
    "generate:token": "node scripts/token.generate.js",
    "generate:certs": "./scripts/certs.generate.sh",
    "generate:swagger": "node scripts/swagger.generate.js > docs/api.swagger.json",
    "generate:postman": "openapi2postmanv2 -s docs/api.swagger.json -o docs/api.postman.json",
    "docker:start": "COMPOSE_PROFILES=all docker compose up --detach && npm run docker:logs",
    "docker:stop": "COMPOSE_PROFILES=all docker compose down",
    "docker:restart": "npm run docker:stop && npm run docker:start",
    "docker:db:start": "COMPOSE_PROFILES=db docker compose up --detach",
    "docker:db:stop": "COMPOSE_PROFILES=db docker compose down",
    "docker:db:restart": "COMPOSE_PROFILES=db npm run docker:stop && npm run docker:start",
    "docker:logs": "docker logs -f app",
    "docker:config": "docker compose --env-file .env/.common.env --env-file .env/.docker.env config",
    "prepare": "husky install"
  },
  "keywords": [
    "backend",
    "paramlabs",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@imtbl/core-sdk": "^1.0.1",
    "@sendgrid/mail": "^7.7.0",
    "ajv": "^8.12.0",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^2.1.1",
    "axios": "^1.3.2",
    "bcryptjs": "^2.4.3",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "glob": "^8.1.0",
    "googleapis": "^110.0.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "mongoose": "^6.9.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nunjucks": "^3.2.3",
    "pg": "^8.9.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.28.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^8.0.3",
    "hygen": "^6.2.11",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "openapi-to-postmanv2": "^4.9.0",
    "prettier": "^2.8.3",
    "sinon": "^15.0.1",
    "supertest": "^6.3.3",
    "swagger-jsdoc": "^6.2.8",
    "yaml": "^2.2.1"
  },
  "_moduleAliases": {
    "~": "src"
  },
  "volta": {
    "node": "16.19.0"
  }
}
